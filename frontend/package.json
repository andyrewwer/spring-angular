{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --output-path=dist/static",
    "build": "ng build --prod --progress=false --base-href=/  --output-path=dist/static",
    "buildWatch": "ng build --progress=false --base-href=/ --watch=true --output-path=dist/static",
    "test": "ng test --code-coverage --watch=false --progress=false",
    "lint": "ng lint",
    "e2e": "ng e2e --webdriver-update false",
    "postinstall": "ncu --packageFile package.json",
    "check-for-dependency-updates": "ncu --packageFile package.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.2",
    "@angular/cdk": "^8.0.1",
    "@angular/common": "^8.0.2",
    "@angular/compiler": "^8.0.2",
    "@angular/core": "^8.0.2",
    "@angular/forms": "^8.0.2",
    "@angular/material": "^8.0.1",
    "@angular/platform-browser": "^8.0.2",
    "@angular/platform-browser-dynamic": "^8.0.2",
    "@angular/router": "^8.0.2",
    "bootstrap": "^4.3.1",
    "core-js": "^3.1.4",
    "hammerjs": "^2.0.8",
    "jquery": "^3.4.1",
    "ngx-bootstrap": "^4.3.0",
    "npm-check-updates": "^3.1.11",
    "rxjs": "^6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "^8.0.2",
    "@types/node": "~12.0.10",
    "codelyzer": "^5.0.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.4.5"
  }
}
